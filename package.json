    "@vuepress/plugin-docsearch": "2.0.0-alpha.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "vue": "^3.2.37",
    "vuepress": "2.0.0-beta.49"
  },
  "lint-staged": {
    "**/*.{js,vue}": "eslint --cache --fix --no-ignore",
